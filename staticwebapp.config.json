{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,js,json,png,jpg,gif,svg,ico}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  }
}